{
    "name": "andrehebben/performan",
    "description": "Andre Hebben's Homepage and Goal tracker",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "resume/portfolio at the front end ,   a goaltracker / information system on the backend",
        "personal",
        "zf3"
    ],
    "homepage": "https://andrehebben.nl/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "laminas/laminas-component-installer": "^1.0 || ^0.7 || ^1.0.0-dev@dev",
        "laminas/laminas-mvc": "^3.1.1",
        "laminas/laminas-mvc-form": "^1.0",
        "laminas/laminas-cache": "^2.7.1",
        "laminas/laminas-json": "^3.0",
        "laminas/laminas-mail": "^2.10.0",
        "laminas/laminas-mvc-console": "^1.1.10",
        "laminas/laminas-mvc-i18n": "^1.0",
        "laminas/laminas-mvc-plugins": "^1.0",
        "laminas/laminas-navigation": "^2.8.1",
        "laminas/laminas-serializer": "^2.8",
        "laminas/laminas-twitter": "^3.0",
        "doctrine/doctrine-orm-module": "^2.1",
        "kokspflanze/zfc-twig": "dev-laminas",
        "rwoverdijk/assetmanager": "^1.7",
        "squizlabs/php_codesniffer": "*",
        "twig/twig": "^3.0",
        "laminas/laminas-authentication": "^2.7",
        "laminas/laminas-captcha": "^2.9",
        "laminas/laminas-dependency-plugin": "^1.0",
        "laminas/laminas-session": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Admin\\": "module/Admin/src/",
            "Application\\": "module/Application/src/",
            "User\\": "module/User/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AdminTest\\": "module/Admin/test/",
            "ApplicationTest\\": "module/Application/test/",
            "UserTest\\": "module/User/test/"
        }
    },
    "extra": [],
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "development-disable": "laminas-development-mode disable",
        "development-enable": "laminas-development-mode enable",
        "development-status": "laminas-development-mode status",
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit"
    },
    "require-dev": {
        "laminas/laminas-test": "^3.3",
        "phpstan/phpstan": "^0.11.16",
        "phpmd/phpmd": "^2.7",
        "phploc/phploc": "^5.0"
    }
}
